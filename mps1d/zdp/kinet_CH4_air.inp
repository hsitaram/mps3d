$ kVT10_O2C3H8 = 1.05d-15*Tgas

#-----------------------------------------------------------------------
#
#  electron impact
#
#-----------------------------------------------------------------------

# rotational excitation
# Phelps database, www.lxcat.net.
E + N2 => E + N2                          !  BOLSIG  N2 -> N2(rot)
E + O2 => E + O2                          !  BOLSIG  O2 -> O2(rot)

# vibrational excitation
# Phelps database, www.lxcat.net.
E + N2 => E + N2(v1)                      !  BOLSIG  N2 -> N2(v1res)
E + N2 => E + N2(v1)                      !  BOLSIG  N2 -> N2(v1)
E + N2 => E + N2(v2)                      !  BOLSIG  N2 -> N2(v2)
E + N2 => E + N2(v3)                      !  BOLSIG  N2 -> N2(v3)
E + N2 => E + N2(v4)                      !  BOLSIG  N2 -> N2(v4)
E + N2 => E + N2(v5)                      !  BOLSIG  N2 -> N2(v5)
E + N2 => E + N2(v6)                      !  BOLSIG  N2 -> N2(v6)
E + N2 => E + N2(v7)                      !  BOLSIG  N2 -> N2(v7)
E + N2 => E + N2(v8)                      !  BOLSIG  N2 -> N2(v8)
E + O2 => E + O2(v1)                      !  BOLSIG  O2 -> O2(v1res)
E + O2 => E + O2(v1)                      !  BOLSIG  O2 -> O2(v1)
E + O2 => E + O2(v2)                      !  BOLSIG  O2 -> O2(v2res)
E + O2 => E + O2(v2)                      !  BOLSIG  O2 -> O2(v2)
E + O2 => E + O2(v3)                      !  BOLSIG  O2 -> O2(v3)
E + O2 => E + O2(v4)                      !  BOLSIG  O2 -> O2(v4)
# IST-Lisbon database, www.lxcat.net.
E + CH4 => E + CH4(v24)                   !  BOLSIG  CH4 -> CH4(v24)
E + CH4 => E + CH4(v13)                   !  BOLSIG  CH4 -> CH4(v13)

# electronic excitation
# Phelps database, www.lxcat.net.
E + N2 => E + N2(A)                       !  BOLSIG N2 -> @R
   @R = N2(A1)   N2(A2)   N2(A3)
E + N2 => E + N2(B)                       !  BOLSIG N2 -> @R
   @R = N2(B3)   N2(W3)   N2(B'3)
E + N2 => E + N2(a')                      !  BOLSIG N2 -> @R
   @R = N2(au1)  N2(a'1)  N2(w1)
E + N2 => E + N2(C)                       !  BOLSIG N2 -> @R
   @R = N2(C3)   N2(E3)   N2(a''1) 
E + O2 => E + O2(a1)                      !  BOLSIG O2 -> O2(a1)
E + O2 => E + O2(b1)                      !  BOLSIG O2 -> O2(b1)
E + O2 => E + O2*                         !  BOLSIG O2 -> O2*  

# dissociation
# Phelps database, www.lxcat.net.
E + N2 => E + N + N                       !  BOLSIG N2 -> N + N
E + O2 => E + O + O                       !  BOLSIG O2 -> O + O
E + O2 => E + O + O(1D)                   !  BOLSIG O2 -> O + O(1D)
E + O2 => E + O + O(1S)                   !  BOLSIG O2 -> O + O(1S)
# R.K. Janev, D. Reiter, Phys. Plasmas. 9 (9) (2002) 4071-4081.
E + CH4 => E + CH3 + H                    !  BOLSIG CH4 -> CH3 + H
E + CH4 => E + CH2 + H2                   !  BOLSIG CH4 -> CH2 + H2
E + CH4 => E + CH + H2 + H                !  BOLSIG CH4 -> CH + H2 + H
E + CH4 => E + C + 2H2                    !  BOLSIG CH4 -> C + 2H2

# ionization
# Phelps database, www.lxcat.net.
E + N2 => E + E + N2^+                    !  BOLSIG N2 -> N2^+
E + O2 => E + E + O2^+                    !  BOLSIG O2 -> O2^+
# H.C. Straub, D.Lin, et al, J. Chem. Phys. 106 (11) (1997) 4430-4435.
E + CH4 => E + E + CH4^+                  !  BOLSIG CH4 -> CH4^+
E + CH4 => E + E + CH3^+ + H              !  BOLSIG CH4 -> CH3^+ + H
E + CH4 => E + E + CH2^+ + H2             !  BOLSIG CH4 -> CH2^+ + H2

